<view class="index">

  <view class="index-bd">
    <view class="kind-list">
      <block wx:if="{{applicationLog.length<=0}}">
        <view>暂无预约记录</view>
      </block>
      <block wx:for="{{postList}}" wx:for-item="item" wx:key="applicationLog">
        <view id="{{item.id}}"  bindtap="postDetail">
          <text>{{item.postStartAddress}}=>{{item.postEndAddress}}</text>
        </view>
      </block>


    </view>
  </view>
  <button type="primary" bindtap="editPost" formType="submit">新增发布</button>

</view>