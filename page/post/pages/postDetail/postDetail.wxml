<view class="container">
  <view class="page-body">
    <view>{{postData.postStartAddress}}</view>
    <view>{{postData.postEndAddress}}</view>
    <view>{{postData.postStartDateTime}}</view>
  </view>
  <block wx:if="{{applications.length>0}}">
    <block wx:for-items="{{applications}}" wx:key='applicationLog'>
      <view id="{{item.id}}">
        <view>{{item.postStartAddress}}=>{{item.postEndAddress}}</view>
      </view>
    </block>
  </block>
  <block wx:if="{{applications.length<=0}}">
    <view>暂无预约记录</view>
  </block>


  <button bindtap="onCancel">cancel</button>
</view>