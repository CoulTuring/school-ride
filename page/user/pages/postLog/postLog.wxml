<view class="container">
  <view class="page-body">
    <block wx:if="{{postLog && postLog.length>0}}">
      <block wx:for-items="{{postLog}}" wx:key='postLog'>
        <view id="{{item.id}}">
          <view>{{item.postStartAddress}}=>{{item.postEndAddress}}</view>
        </view>
      </block>
    </block>
    <block wx:if="{{postLog && postLog.length<=0}}">
      <view>暂无发布记录</view>
    </block>
  </view>
</view>